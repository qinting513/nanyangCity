<view class="input-cell data-v-2b051cb2"><view class="flex-row title-box data-v-2b051cb2"><block wx:if="{{isNeed}}"><text class="title-star data-v-2b051cb2">*</text></block><view class="cell-title data-v-2b051cb2" style="{{(titleStyle)}}">{{title}}</view></view><view class="cell-content data-v-2b051cb2"><block wx:if="{{type=='select'}}"><picker disabled="{{isDisable}}" range="{{itemList}}" value="{{index}}" start="{{startDate}}" end="{{endDate}}" mode="selector" data-event-opts="{{[['change',[['bindSelect',['$event']]]]]}}" bindchange="__e" class="data-v-2b051cb2"><view class="contentView data-v-2b051cb2"><input disabled="{{true}}" data-field="{{field}}" placeholder="{{placeholder}}" value="{{selectStr!=''?selectStr:initValue}}" class="data-v-2b051cb2"/><image class="image data-v-2b051cb2" src="{{isDisable?'':'/static/images/input_cell_imgs/icon_arrow_down.png'}}"></image></view></picker></block><block wx:else><block wx:if="{{type=='linkBtn'}}"><view class="buttonView data-v-2b051cb2"><input type="{{inputType}}" data-field="{{field}}" placeholder="{{placeholder}}" disabled="true" data-event-opts="{{[['tap',[['clickBtn',['$event']]]]]}}" value="{{inputStr!=''?inputStr:initValue}}" bindtap="__e" class="data-v-2b051cb2"/><image class="image data-v-2b051cb2" mode="widthFix" src="/static/images/input_cell_imgs/icon_right_arrow.png"></image></view></block><block wx:else><block wx:if="{{type=='readOnly'}}"><view class="contentView data-v-2b051cb2" style="{{(contentColor)}}"><input type="{{inputType}}" data-field="{{field}}" placeholder="{{placeholder}}" disabled="true" value="{{inputStr||initValue}}" class="data-v-2b051cb2"/></view></block><block wx:else><block wx:if="{{type=='textView'}}"><view class="contentView data-v-2b051cb2" style="{{(contentColor)}}"><view class="textView data-v-2b051cb2">{{inputStr!=''?inputStr:initValue}}</view></view></block><block wx:else><view class="contentView data-v-2b051cb2"><input type="{{inputType}}" data-field="{{field}}" placeholder="{{placeholder}}" data-event-opts="{{[['input',[['bindInputChange',['$event']]]]]}}" value="{{initValue}}" bindinput="__e" class="data-v-2b051cb2"/><image class="image data-v-2b051cb2" mode="widthFix" src="{{inputStr==''&&initValue==''?'/static/images/input_cell_imgs/icon_clear_normal.png':'/static/images/input_cell_imgs/icon_clear.png'}}" data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" bindtap="__e"></image></view></block></block></block></block></view></view>